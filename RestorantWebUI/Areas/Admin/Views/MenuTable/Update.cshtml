@model UpdateMenuTableDto

@{
    ViewData["Title"] = "Update";
    Layout = "~/Areas/Admin/Views/AdminLayout/Index.cshtml";
}


<div class="content">
    <div class="container-fluid">
        <h4 class="page-title">Table Process</h4>
        <div class="row">
            <div class="col-md-12">
                <form method="post">
                    <div class="card">
                        <div class="card-header">
                            <div class="card-title">Update Table</div>
                        </div>
                        <div class="card-body">
                            <div class="form-group">
                                <input type="hidden" class="form-control" asp-for="MenuTableId">
                                <label for="categoryname">Name</label>
                                <input type="text" class="form-control" asp-for="MenuTableName">
                            </div>
                            <div class="form-group">
                                <label for="categoryname">Phone</label>
                                <input type="text" class="form-control" asp-for="MenuTableStatus">
                            </div>
                        </div>
                        <div class="card-action">
                            <button class="btn btn-primary">Update Table</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>




